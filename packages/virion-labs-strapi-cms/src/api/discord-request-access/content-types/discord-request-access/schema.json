    },
    "email": {
      "type": "email",
      "required": true
    },
    "discord_user_id": {
      "type": "string",
      "required": true
    },
    "discord_username": {
      "type": "string",
      "required": true
    },
    "request_status": {
      "type": "enumeration",
      "required": true,
      "default": "pending",
      "enum": [
        "pending",
        "approved",
        "rejected"
      ]
    },
    "guild_id": {
      "type": "string",
      "required": true
    }
  }
}
