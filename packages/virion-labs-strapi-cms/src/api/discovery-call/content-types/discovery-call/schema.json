{
  "kind": "collectionType",
  "collectionName": "discovery_calls",
  "info": {
    "singularName": "discovery-call",
    "pluralName": "discovery-calls",
    "displayName": "Discovery Call"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "users-permissions": {
      "enabled": true
    }
  },
  "attributes": {
    "scheduled_at": { "type": "datetime", "required": true },
    "timezone": { "type": "string" },
    "duration_minutes": { "type": "integer", "default": 30 },
    "google_event_id": { "type": "string" },
    "meeting_url": { "type": "string" },
    "status": {
      "type": "enumeration",
      "default": "scheduled",
      "enum": ["scheduled", "completed", "cancelled"]
    },
    "lead": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::client-lead.client-lead",
      "inversedBy": "discovery_calls"
    }
  }
}
