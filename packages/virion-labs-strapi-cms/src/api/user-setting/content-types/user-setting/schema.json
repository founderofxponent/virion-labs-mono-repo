{
  "kind": "collectionType",
  "collectionName": "user_settings",
  "info": {
    "singularName": "user-setting",
    "pluralName": "user-settings",
    "displayName": "User Setting"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "bio": {
      "type": "richtext"
    },
    "phone_number": {
      "type": "string"
    },
    "twitter_handle": {
      "type": "string"
    },
    "instagram_handle": {
      "type": "string"
    },
    "youtube_channel": {
      "type": "string"
    },
    "discord_username": {
      "type": "string"
    },
    "website_url": {
      "type": "string"
    },
    "email_notifications_new_referral": {
      "type": "boolean",
      "default": true
    },
    "email_notifications_link_clicks": {
      "type": "boolean",
      "default": false
    },
    "email_notifications_weekly_reports": {
      "type": "boolean",
      "default": true
    },
    "email_notifications_product_updates": {
      "type": "boolean",
      "default": true
    },
    "push_notifications_new_referral": {
      "type": "boolean",
      "default": false
    },
    "push_notifications_link_clicks": {
      "type": "boolean",
      "default": false
    },
    "push_notifications_weekly_reports": {
      "type": "boolean",
      "default": false
    },
    "push_notifications_product_updates": {
      "type": "boolean",
      "default": false
    },
    "profile_visibility": {
      "type": "enumeration",
      "enum": [
        "public",
        "private"
      ]
    },
    "show_earnings": {
      "type": "boolean",
      "default": false
    },
    "show_referral_count": {
      "type": "boolean",
      "default": true
    },
    "webhook_url": {
      "type": "string"
    },
    "webhook_events": {
      "type": "json"
    },
    "api_key": {
      "type": "string"
    },
    "api_key_test": {
      "type": "string"
    },
    "api_key_regenerated_at": {
      "type": "datetime"
    },
    "theme": {
      "type": "enumeration",
      "default": "system",
      "enum": [
        "light",
        "dark",
        "system"
      ]
    },
    "language": {
      "type": "string",
      "default": "en"
    },
    "timezone": {
      "type": "string",
      "default": "UTC"
    },
    "currency": {
      "type": "string",
      "default": "USD"
    },
    "two_factor_enabled": {
      "type": "boolean",
      "default": false
    },
    "login_notifications": {
      "type": "boolean",
      "default": true
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "user_setting"
    }
  }
}
