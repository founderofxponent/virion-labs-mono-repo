{
  "kind": "collectionType",
  "collectionName": "campaign_onboarding_fields",
  "info": {
    "singularName": "campaign-onboarding-field",
    "pluralName": "campaign-onboarding-fields",
    "displayName": "Campaign Onboarding Field"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "field_key": {
      "type": "string",
      "required": true
    },
    "field_label": {
      "type": "string",
      "required": true
    },
    "field_type": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "text",
        "email",
        "number",
        "boolean",
        "url",
        "select",
        "multiselect"
      ]
    },
    "field_placeholder": {
      "type": "string"
    },
    "field_description": {
      "type": "string"
    },
    "field_options": {
      "type": "json"
    },
    "is_required": {
      "type": "boolean",
      "default": false
    },
    "is_enabled": {
      "type": "boolean",
      "default": true
    },
    "sort_order": {
      "type": "integer",
      "default": 0
    },
    "validation_rules": {
      "type": "json"
    },
    "discord_integration": {
      "type": "json"
    },
    "step_number": {
      "type": "integer",
      "default": 1,
      "required": true
    },
    "step_role_ids": {
      "type": "json"
    },
    "campaign": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::campaign.campaign",
      "inversedBy": "campaign_onboarding_fields"
    }
  }
}
