{
  "kind": "collectionType",
  "collectionName": "onboarding_responses",
  "info": {
    "singularName": "onboarding-response",
    "pluralName": "onboarding-responses",
    "displayName": "Onboarding Response",
    "description": "Stores user responses from campaign onboarding."
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "discordUserId": {
      "type": "string",
      "required": true
    },
    "discordUsername": {
      "type": "string"
    },
    "fieldKey": {
      "type": "string",
      "required": true
    },
    "fieldValue": {
      "type": "text"
    },
    "isCompleted": {
      "type": "boolean",
      "default": false
    },
    "campaign": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::campaign.campaign"
    },
    "referral_link": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::referral-link.referral-link"
    }
  }
}
